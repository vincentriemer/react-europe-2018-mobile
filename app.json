{
  "expo": {
    "name": "ReactEurope",
    "description":
      "Official react-europe app for the May 2018 conference, May 17-18 in Paris, France. Read the source or get the app store and play store links on Github: https://github.com/react-europe/react-europe-2018-mobile. Credits to Brent Vatne and Expo.",
    "slug": "reacteurope",
    "privacy": "public",
    "version": "6.4.2",
    "primaryColor": "#4d5fab",
    "icon": "./src/assets/icon-ios.png",
    "orientation": "portrait",
    "sdkVersion": "25.0.0",
    "scheme": "reacteurope",
    "splash": {
      "image": "./src/assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#4d5fab"
    },
    "android": {
      "package": "org.reacteurope.app",
      "icon": "./src/assets/icon-android.png",
      "versionCode": 5,
      "permissions": ["CAMERA", "RECORD_AUDIO"]
    },
    "ios": {
      "bundleIdentifier": "reacteurope",
      "supportsTablet": true,
      "loadJSInBackgroundExperimental": true
    }
  }
}
