{
  "name": "ReactEurope2018",
  "private": true,
  "main": "./node_modules/expo/AppEntry.js",
  "jest": {
    "preset": "jest-expo"
  },
  "scripts": {
    "start": "exp start",
    "test": "jest",
    "devtools": "react-devtools",
    "prebuild:dom": "rimraf dom/dist && mkdir -p dom/dist",
    "build:dom": "NODE_ENV=production yarn build:dom:native && yarn build:dom:js",
    "build:dom:native": "react-native bundle --config $(pwd)/rn-cli.config.js --dev false --entry-file ./dom/bootstrap.js --bundle-output ./dom/dist/bootstrap.bundle --platform dom",
    "build:dom:js": "react-native bundle --config $(pwd)/rn-cli.config.js --dev false --entry-file ./dom/entry.js --bundle-output ./dom/dist/entry.bundle --platform dom --assets-dest ./dom/dist",
    "postbuild:dom": "cp ./dom/index.html ./dom/dist/index.html && cp ./dom/now.json ./dom/dist/now.json"
  },
  "dependencies": {
    "@expo/vector-icons": "^6.3.1",
    "babel-polyfill": "^6.26.0",
    "babel-preset-expo": "^4.0.0",
    "babel-runtime": "^6.26.0",
    "crypto-js": "^3.1.9-1",
    "exp": "^52.0.3",
    "expo": "^27.0.0",
    "fbemitter": "^2.1.1",
    "fontfaceobserver": "^2.0.13",
    "global": "^4.3.2",
    "hammerjs": "^2.0.8",
    "hoist-non-react-statics": "^2.3.1",
    "jsqr": "^1.0.4",
    "lodash": "^4.17.4",
    "moment": "2.21.0",
    "moment-timezone": "^0.5.14",
    "path": "^0.12.7",
    "prettier": "^1.11.1",
    "prop-types": "^15.6.1",
    "react": "16.3.1",
    "react-native": "~0.55.0",
    "react-native-animatable": "^1.2.4",
    "react-native-dom": "file:.yalc/react-native-dom",
    "react-native-drawer-layout-polyfill": "^2.0.0",
    "react-native-easy-markdown": "^1.1.9",
    "react-native-fade-in-image": "file:local-forks/react-native-fade-in-image",
    "react-native-gravatar": "^1.0.2",
    "react-native-paper": "file:local-forks/react-native-paper",
    "react-native-qrcode": "^0.2.6",
    "react-native-read-more-text": "^1.0.0",
    "react-native-resource-saving-container": "^1.0.2",
    "react-navigation": "1.5.11",
    "urql": "^0.2.0"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "babel-plugin-transform-builtin-classes": "^0.6.1",
    "babel-plugin-transform-remove-console": "^6.9.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-react-native": "^4.0.0",
    "eslint": "^4.5.0",
    "eslint-plugin-react": "^7.3.0",
    "eslint-plugin-react-native": "^3.0.1",
    "flow-bin": "^0.68.0",
    "jest-expo": "~27.0.0",
    "react-devtools": "^3.0.0",
    "react-test-renderer": "16.3.1"
  }
}
